<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Body</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
      <td><%= link_to 'Show', book_path(book.id) %></td>
      <td><%= link_to 'Edit', edit_book_path(book.id) %></td>
      <td><a data-confirm="Are you sure?" class="destroy_273457" rel="nofollow" data-method="delete" href="/books/273457">Destroy</a></td>    </tr>
    <% end %>
  </tbody>
</table>
